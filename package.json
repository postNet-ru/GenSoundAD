{
  "name": "gensoundad",
  "private": true,
  "version": "0.0.6",
  "description": "Генератор аудио контента с рекламными вставками - профессиональный инструмент для создания многочасовых плейлистов",
  "author": "Noradrenalin Team <dev@noradrenalin.team>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Noradrenalin-team/GenSoundAD.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:fast": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "type-check": "tsc --noEmit",
    "prepare-release": "npm run type-check && npm run lint && npm run build",
    "build:windows": "tauri build --target x86_64-pc-windows-msvc",
    "build:macos": "tauri build --target x86_64-apple-darwin",
    "build:linux": "tauri build --target x86_64-unknown-linux-gnu"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@gravity-ui/date-utils": "^2.5.6",
    "@tauri-apps/api": "^2.7.0",
    "@tauri-apps/plugin-process": "^2.3.0",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@ffmpeg/core": "^0.12.10",
    "@gravity-ui/date-components": "^3.0.1",
    "@gravity-ui/icons": "^2.15.0",
    "@gravity-ui/uikit": "^7.1.1",
    "@tauri-apps/cli": "^2.7.0",
    "@types/node": "^24.2.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
